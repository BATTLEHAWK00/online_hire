{% extends './partials/layouts/basic.njk' %}
{% block content %}
<div class="section">
    <div class="section-header">
        <h1>错误:{{ error.status }}</h1>
    </div>
    <div class="section-content">
        <h2>{{ error.message | d('服务器错误') }}</h2>
    </div>
    <div class="section-footer">
        <pre>{{ error.stack }}</pre>
    </div>
</div>
{% endblock %}