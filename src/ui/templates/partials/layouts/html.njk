<!doctype html>
<!--suppress HtmlUnknownTarget, JSUnresolvedLibraryURL -->
<html lang="zh" page-name="{{ pageName }}">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>{{ pageTitle }}</title>
    <script>
      window.UIContext = JSON.parse('{{ UIContext | dump | d('{}') | safe }}');
    </script>
    <script src="{{ staticFilesMap['/main.js'] | d('/main.js') }}"></script>
    <script src="{{ staticFilesMap['/chunk-vendors.js'] | d('/chunk-vendors.js') }}"></script>
    <link rel="stylesheet" href="{{ staticFilesMap['/main.css'] | d('/main.css') }}">
    <script src="https://cdn.jsdelivr.net/npm/vue@next"></script>
    {% block head %} {% endblock %}
</head>
<body>
{% block header %} {% endblock %}
<div class="app">
    {% block content %} {% endblock %}
</div>
{% block footer %} {% endblock %}
</body>
</html>